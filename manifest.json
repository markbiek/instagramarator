{
    "manifest_version": 2,

    "name": "Instagramarator",
    "description": "This extension makes it easier to download an Instagram image.",
    "version": "1.0",
    "permissions": [
        "tabs", 
        "https://instagram.com/*",
        "https://www.instagram.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://instagram.com/*","https://www.instagram.com/*"],
            "js": ["jquery.min.js", "inject.js"]
        }
    ]
}

